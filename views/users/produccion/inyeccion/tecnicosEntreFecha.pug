        function initChart() {
            // Crea el gráfico en la carga de la página
            let ctx = document.getElementById('myChart').getContext("2d");
            let chart = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: [],
                    datasets: [{
                        label: 'Scrap por Tecnico',
                        data: [], // Los datos se actualizarán más tarde
                        borderWidth: 1
                    },
                    {
                        type: 'line',
                        label: 'Objetivo',
                        data: [],
                        borderWidth: 1
                    }]
                },
                options: {
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            beginAtZero: true
                        }
                    }
                },
                plugins:[ChartDataLabels]
            });

            return chart;
        }
        function initChart2() {
            // Crea el gráfico en la carga de la página
            let ctx = document.getElementById('myChart2').getContext("2d");
            let chart2 = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: [],
                    datasets: [{
                        label: 'Ruedas Ok Por Tecnico',
                        data: [], // Los datos se actualizarán más tarde
                        borderWidth: 1
                    },
                    ]
                },
                options: {
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            beginAtZero: true,
                            
                        },
                        x:{
                          beginAtZero: true,
                          stacked:true
                        }
                    }
                },
                plugins:[ChartDataLabels]
            });

            return chart2;
        }
        function updateChart(){
          function fecha(x){
            let f = new Date(x);
            let dia=f.getDate();
            let mes = f.getMonth()+1;
            let año = f.getUTCFullYear();
            let fecha = dia+"/"+mes+"/"+año;
            return fecha
          }
            var fini = document.getElementById('fini').value;
            var ffin = document.getElementById('ffin').value;
        
            var tabla = document.getElementById('tabla');
            
            
          fetch(`tecnicosInyeccion?fini=${fini}&ffin=${ffin}`)
            .then(response => response.json())
            .then(datos =>{
                console.log(datos)
                let chart = Chart.instances[0];
                chart.options.onClick = function(evt, elements) {
                  if (elements.length) {
                    // Obtén el índice del elemento clickeado
                    const index = elements[0].index;

                    // Elimina la etiqueta y los datos correspondientes
                    chart.data.labels.splice(index, 1);
                    chart.data.datasets.forEach((dataset) => {
                      dataset.data.splice(index, 1);
                    });

                    // Actualiza el gráfico
                    chart.update();
                  }
                };
                chart.data = {
                          labels: datos.map(e => e.Tecnico),
                          
                          datasets: [{
                            data: datos.map(e => e.Scrap),
                            label:'Scrap',
                            type:'bar',
                            
                            datalabels:{
                              align:'top',
                              font:{
                                weight:'bold'
                              },
                              formatter: function(value, context) {
                                  return Number(value*100).toFixed(2) + '%';
                                }
                            }
                          },
                          {
                            data: datos.map(e => 0.045),
                            label:'Objetivo',
                            type:'line',
                            
                            datalabels:{
                              display:false
                            }
                          }],
                          
                          
                        }
                    chart.update();
                    let ordenado=[...datos].sort((a, b) => b.ok - a.ok);
                    let ordProm=[...datos].sort((a, b)=> b.promedio - a.promedio);
                    let chart2 = Chart.instances[1];
                    chart2.data = {
                          labels: ordProm.map(e => e.Tecnico),
                          
                          datasets: [{
                            data: ordProm.map(e => e.ok),
                            label:'Ruedas ok totales',
                            type:'bar',
                            
                            datalabels:{
                              offset:'top',
                              anchor:'end',
                              font:{
                                weight:'bold',
                                
                              }
                            }
                           
                          },{
                            data: ordProm.map(e => e.promedio),
                            label:'Promedio ruedas maquina',
                            type:'bar',
                            
                            datalabels:{
                              offset:'top',
                              anchor:'end',
                              font:{
                                weight:'bold',
                                
                              }
                            }
                           
                          },
                          
                          ],
                          
                          
                        }  
                    chart2.update(); 
                        
                       

                  

                  tabla.innerHTML ="";
                  for(let i=0; i<ordProm.length;i++){
                                          
                      tabla.innerHTML += `
                    <tr>
                      <td>${ordProm[i].Tecnico}</td>
                      <td>${ordProm[i].Golpes}</td>
                      <td>${ordProm[i].Isla}</td>
                      <td>${ordProm[i].RX}</td>
                      <td>${ordProm[i].ok}</td>
                      <td>${ordProm[i].maquinas}</td>
                      <td>${ordProm[i].promedio}</td>
                      <td>${(ordProm[i].Scrap*100).toFixed(2)}%</td>
                    </tr>`
                    
                    
                  }
                  
                    
            })
          
                  

        }
        
      


            
    
